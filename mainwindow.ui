     <property name="styleSheet">
      <string notr="true">/* QTableView Styling */
QTableView {
    border-style: solid;
    background-color: #3d3d3d;  /* Dark background */
    color: #ffffff;  /* White text */
    border-radius: 7px;  /* Rounded corners */
    border-width: 2px;  /* Set border width */
    border-color: #00ff00;  /* Green border */
    gridline-color: #00ff00;  /* Green gridlines */
    selection-background-color: #2d89ef;  /* Blue background when selected */
    selection-color: #ffffff;  /* White text when selected */
}

/* Header Styling */
QHeaderView::section {
    background-color: #3d3d3d;  /* Dark background for header */
    color: #ffffff;  /* White text for headers */
    border-style: solid;
    border-width: 2px;
    border-color: #00ff00;  /* Green border */
    padding: 8px;
    font-weight: bold;  /* Bold header text */
}

/* Hover effect for header */
QHeaderView::section:hover {
    background-color: #2d89ef;  /* Blue background on hover */
    color: #ffffff;  /* Keep white text */
}

/* Odd rows */
QTableView::item {
    background-color: #2b2b2b;  /* Darker background for rows */
    color: #ffffff;  /* White text */
}

/* Even rows */
QTableView::item:alternate {
    background-color: #3d3d3d;  /* Slightly lighter for alternate rows */
}

/* Hover effect for rows */
QTableView::item:hover {
    background-color: #505050;  /* Lighter gray on hover */
    color: #ffffff;  /* Keep text white */
}

/* Selected row */
QTableView::item:selected {
    background-color: #2d89ef;  /* Blue background for selected rows */
    color: #ffffff;  /* White text */
}
</string>
