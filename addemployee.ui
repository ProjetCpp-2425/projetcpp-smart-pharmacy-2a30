   <property name="styleSheet">
    <string notr="true">/* QDateEdit Styling */
QDateEdit {
    border-style: solid;
    background-color: #3d3d3d;  /* Same dark background */
    color: #fff;  /* White text */
    border-radius: 7px;  /* Rounded corners */
    border-width: 2px;  /* Set border width */
    border-color: #00ff00;  /* Green border */
    padding: 6px;  /* Padding inside the date edit */
    outline: none; /* Remove default focus outline */
    /* Align text to the center */
    qproperty-alignment: 'AlignVCenter | AlignHCenter';
}

/* Styling the drop-down arrow button of QDateEdit */
QDateEdit::drop-down {
    width: 25px; /* Width of the drop-down arrow button */
    border: none;  /* Remove border */
    background-color: #3d3d3d;  /* Same background as QDateEdit */
}

/* Drop-down arrow icon in QDateEdit */
QDateEdit::down-arrow {
    image: url(:/icons/arrow_down.png);  /* Custom down arrow icon (you can replace it with any icon path) */
    width: 10px;
    height: 10px;
}

/* Hover effect */
QDateEdit:hover {
    color: #ccc;  /* Light gray text on hover */
    background-color: qlineargradient(spread:pad, x1:0.517, y1:0, x2:0.517, y2:1, stop:0 rgba(45, 45, 45, 255), stop:1 rgba(29, 29, 29, 255));  /* Darker background */
    border-color: #2d89ef;  /* Blue border on hover */
}

/* Focused effect (when clicked) */
QDateEdit:focus {
    background-color: qlineargradient(spread:pad, x1:0.517, y1:0, x2:0.517, y2:1, stop:0 rgba(29, 29, 29, 255), stop:1 rgba(45, 45, 45, 255));  /* Slightly darker background */
    border-color: #da532c;  /* Orange border on focus */
}

/* Disabled QDateEdit */
QDateEdit:disabled {
    background-color: #2b2b2b;  /* Darker background for disabled state */
    color: #aaaaaa;  /* Gray text when disabled */
    border-color: #555555;  /* Lighter border when disabled */
}
</string>
